{
  "css.validate": false,
  "files.autoSave": "off",
  "workbench.startupEditor": "terminal",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "files.exclude": {
    "**/tmp/**": true,
    "**/dist/**": true,
    "**/node_modules/**": true,
    "**/.next/**": true,
    "**/next-env.d.ts": true
  },
  "search.exclude": {
    "**/tmp/**": true,
    "**/dist/**": true,
    "**/node_modules/**": true,
    "**/.next/**": true,
    "**/next-env.d.ts": true
  },
  "files.watcherExclude": {
    "**/tmp/**": true,
    "**/dist/**": true,
    "**/node_modules/**": true,
    "**/.next/**": true,
    "**/next-env.d.ts": true
  },
  "prisma.fileWatcher": false
}
